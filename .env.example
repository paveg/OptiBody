# OptiBody Environment Variables Template
# Copy this file to .env and fill in your actual values

# Authentication Secret (REQUIRED for production)
# Generate with: openssl rand -base64 32
SESSION_SECRET="your-super-secure-session-secret-here-32-chars-minimum"

# Environment
NODE_ENV="development"

# Cloudflare Development Configuration (optional for local development)
CLOUDFLARE_ACCOUNT_ID="your-cloudflare-account-id"
CLOUDFLARE_DATABASE_ID="your-d1-database-id"
CLOUDFLARE_D1_TOKEN="your-d1-api-token"